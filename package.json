{
  "name": "neurallog-log-server",
  "version": "1.0.0",
  "description": "Log server for the NeuralLog system with zero-knowledge storage adapter support",
  "main": "dist/server.js",
  "scripts": {
    "generate-api-types": "node scripts/generate-log-types.js",
    "prebuild": "npm run generate-api-types",
    "build": "tsc",
    "test": "jest",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts'",
    "test:redis": "powershell -File ./test-redis.ps1",
    "test:redis:unix": "bash ./test-redis.sh",
    "test:e2e": "ts-node tests/e2e/docker-e2e-test.ts",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node src/server.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "docker:build": "docker build -t neurallog/log-server:latest .",
    "docker:run": "docker run -p 3030:3030 -v neurallog-log-server-data:/app/data neurallog/log-server:latest",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "k8s:apply": "kubectl apply -k k8s",
    "k8s:delete": "kubectl delete -k k8s"
  },
  "keywords": [
    "neurallog",
    "logger",
    "server",
    "logging",
    "storage",
    "adapter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@neurallog/client-sdk": "^0.1.19",
    "@neurallog/shared": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.4.9",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nedb": "^1.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/nedb": "^1.8.16",
    "@types/node": "^20.17.30",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.1",
    "openapi-typescript-codegen": "^0.29.0",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.0",
    "swagger-typescript-api": "^13.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
